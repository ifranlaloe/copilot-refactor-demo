        ]
    },
    "dependencies": {
        "axios": "^1.6.0"
    },
    "devDependencies": {
        "@types/jest": "^29.5.14",
        "jest": "^29.7.0",
        "nodemon": "^3.1.10",
        "ts-jest": "^29.3.4",
        "ts-node": "^10.0.0",
        "typescript": "^5.0.0"
    }
}
