{
    "name": "client-refactor",
    "version": "0.0.1",
    "private": true,
    "type": "commonjs",
    "scripts": {
        "start": "ts-node src/main.ts",
        "dev": "nodemon src/main.ts",
        "compile": "tsc",
        "test": "npm run compile && jest --config jest.config.ts"
